#include <iostream>
using namespace std;

class arr {
public:
	int num;
};
class mas :arr {
public:
	mas();
	mas(int);
	mas(const mas& k);
	~mas();
  int konec;
  int nachalo;
 int set_nachalo(){
   cin>>nachalo;
 }
 int set_konec(){
   cin>>konec;
 }

	void swap(int place, int palce1);
