};
int main()
{

  int place1, place;
	int size;
	std::cin >> size;
	std::cin >> place >> place1;
	mas masiv(size);
  masiv.set_nachalo();
  masiv.set_konec();

	for (int i = 0; i < size; i++) {
		int k;
		std::cin >> k;
