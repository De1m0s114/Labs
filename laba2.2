mas::mas() {
	size = 0;
	arr* temp = new arr;
	first = temp;
}
mas::mas(int s) {
	size = s;
	arr* temp = new arr[size];
	first = temp;
}
mas::mas(const mas& k) {
	size = k.size;
	first = k.first;
}
mas::~mas() {
	delete[] first;
	first = nullptr;
}
void mas::print() {
	arr* temp = first;
	while (temp < first + size) {
		std::cout << temp->num << " ";
		++temp;
	}
}
void mas::add(int place, int value) {
	if ((place < size)&&(value>nachalo)&&(value<konec)) {
		arr* temp = first + place;
		temp->num = value;
	}
  else{cout<<"vahod za gran";
  }
	
}
void mas::swap(int place, int place1) {
	int a = (first + place)->num;
	(first + place)->num = (first + place1)->num;
	(first + place1)->num = a;
}
